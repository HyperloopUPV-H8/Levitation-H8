% 
% cargar la tabla como table=readtable(csvfile)
%
function [v, t] = plot_gui(csvfile1, csvfile2, csvfile3)

switch nargin
    case 1:
        table1 = readtable(csvfile1);
        [t, v] = vectors(table1);

        plot(t, v)
        title("Recorded data")
        xlabel("Time")
        ylabel("Value")

    case 2:
        table1 = readtable(csvfile1);
        [t, v] = vectors(table1);
        

        table2 = readtable(csvfile2);
        [t, v] = vectors(table1);

end

end

function [t, v] = vectors(table)

    n = height(table1)-1;
    t = []; 
    v = [];

    for i = 1:1:n
        value_raw = string(table.Var5(i));
        value = str2num(extractAfter(value_raw,","));
        time = table.Var2(i);
        t = vertcat(t,time);
        v = vertcat(v,value);
    end

end